{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": ["https://script.google.com/"],
    "content_scripts": [
        {
            "js": [
                "popup.js"
            ],
            "matches": [
                "https://portal.mc.chitose.ac.jp/portal2/*"
            ]
        }
    ]
}